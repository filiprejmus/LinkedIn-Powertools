{
  "name": "cloudsquid powertools",
  "manifest_version": 3,
  "version": "1.0",
  "description": "A Chrome extension that streamlines LinkedIn workflows",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "cloudsquid.png",
    "default_popup": "popup.html",
    "default_title": "LinkedIn power tools by cloudsquid"
  }
}
